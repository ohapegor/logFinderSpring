<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="error">
    <Appenders>
        <Console name="STDOUT">
            <!--<PatternLayout pattern="%d [%p] %c; Message:%m%n"/>-->
            <PatternLayout pattern="Log4j2 %c [%p] Message:%m%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="info">
            <AppenderRef ref="STDOUT"/>
        </Root>
    </Loggers>
</Configuration>